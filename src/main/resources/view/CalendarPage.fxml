<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.Label?>

<?import javafx.scene.layout.TilePane?>
<StackPane fx:id="calendarPage" xmlns:fx="http://javafx.com/fxml/1">
    <VBox minWidth="340" prefWidth="340" VBox.vgrow="ALWAYS">
        <padding>
            <Insets top="10" right="10" bottom="10" left="10"/>
        </padding>
        <Label text="Calendar"></Label>
        <VBox fx:id="calendarContainer" styleClass="calendar-container">
            <Label fx:id="yearLabel" styleClass="year-label"></Label>
            <Label fx:id="monthLabel" styleClass="month-label"></Label>
            <TilePane fx:id="calendarGridView" styleClass="grid-view"></TilePane>
        </VBox>
    </VBox>
</StackPane>
